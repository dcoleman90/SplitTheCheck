<p id="notice"><%= notice %></p>
<table id="show_table">
	<tbody>
      <tr>
        <td>
        	<dl>
        		<dt>
        			<%= @restaurant.name %>
        		</dt>
        		<dd>
        			<%= @restaurant.address %></br>
        		</dd>
        		<dd>	
        			<%= @restaurant.get_city_state_zip %>
        		</dd>
        	</dl>
        </td>
        
        <td class="votes">
        	<%= image_tag("thumbs-up.png", 
        		alt: "Thumbs Up", size: "30x30") %></br>
        	<%= @restaurant.up_votes %>
        </td>
        
        <td class="votes">
        	<%= image_tag("thumbs-down.png", 
        		alt: "Thumbs Down", size: "30x30") %></br>
        	<%= @restaurant.down_votes %>
        	<!-- preface image_tag with link_to when needed -->
        </td>   
	</tbody>
</table>

<div id="partial_links">
	<%= link_to 'Edit', edit_restaurant_path(@restaurant) %> |
	<%= link_to 'Back', restaurants_path %>
</div>
